<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <handlers>
            <!-- script Processor 的值来自命令行工具 wfastcgi-enable -->
            <add name="Python FastCGI"
           path="*"
           verb="*"
           modules="FastCgiModule"           
           scriptProcessor="D:\Project\project-backend-origin\venv\Scripts\python.exe|D:\Project\project-backend-origin\venv\Lib\site-packages\wfastcgi.py"
           resourceType="Unspecified"
           requireAccess="Script" />
        </handlers>
        <httpProtocol>
            <customHeaders>
                <add name="Access-Control-Allow-Methods" value="OPTIONS,POST,GET"/>
                <add name="Access-Control-Allow-Headers" value="x-requested-with"/>
                <add name="Access-Control-Allow-Origin" value="*" />
            </customHeaders>
        </httpProtocol>
    </system.webServer>
    <appSettings>
        <!-- Required settings -->
        <!-- 在这里指定Falsk app在模块中的具体位置 -->
        <add key="WSGI_HANDLER" value="app.app" />
        <add key="PYTHONPATH" value="D:\Project\project-backend-origin" />
        <!-- Optional settings -->
        <!-- 需要先创建日志目录，否则报错 -->
        <add key="WSGI LOG" value="C:\logs\oboeqa web.log" />
        <add key="WSGI RESTART FILE REGEX" value="" />
    </appSettings>
</configuration>
